namespace NSTest;

entrypoint function abortOk(): Int {
    if(1 < 2) {
        return 1;
    }
    else {
        abort;
    }
}
entrypoint function abortFail(): Int {
    if(1 == 2) {
        return 1;
    }
    else {
        abort;
    }
}
entrypoint function assertOk(): Int {
    assert 1 < 2;
    return 1;
}
entrypoint function assertFail(): Int {
    assert 1 > 2;
    return 1;
}
entrypoint function checkOk(): Int {
    check 1 < 2;
    return 1;
}
entrypoint function checkFail(): Int {
    check 1 > 2;
    return 1;
}
