[
    {
        "src": "literals.bsq",
        "tests": [
            {
                "kind": "compile"
            },
            {
                "kind": "aot",
                "entrypoint": "literalNone",
                "expected": "none"
            },
            {
                "kind": "aot",
                "entrypoint": "literalTrue",
                "expected": "true"
            },
            {
                "kind": "aot",
                "entrypoint": "literalFalse",
                "expected": "false"
            },
            {
                "kind": "aot",
                "entrypoint": "literal1",
                "expected": "1"
            },
            {
                "kind": "aot",
                "entrypoint": "literal3",
                "expected": "3"
            },
            {
                "kind": "aot",
                "entrypoint": "literalEmptyString",
                "expected": "\"\""
            },
            {
                "kind": "aot",
                "entrypoint": "literalHello",
                "expected": "\"hello\""
            },
            {
                "kind": "symtest",
                "entrypoint": "literal1"
            },
            {
                "kind": "symexec",
                "entrypoint": "literal1",
                "expected": "1"
            },
            {
                "kind": "symexec",
                "entrypoint": "literalFalse",
                "expected": "false"
            }
        ]
    },
    {
        "src": "tuples.bsq",
        "tests": [
            {
                "kind": "compile"
            },
            {
                "kind": "aot",
                "entrypoint": "emptyTuple",
                "expected": "[]"
            },
            {
                "kind": "aot",
                "entrypoint": "oneTuple",
                "expected": "[1]"
            },
            {
                "kind": "aot",
                "entrypoint": "fourTuple",
                "expected": "[1, 2, none, true]"
            },
            {
                "kind": "aot",
                "entrypoint": "nestedTuples",
                "expected": "[1, [none, []]]"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndex",
                "expected": "1"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexOpt",
                "expected": "none"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexBailout",
                "args": ["true"],
                "expected": "3"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexBailout",
                "args": ["false"],
                "expected": "none"
            },
            {
                "kind": "symtest",
                "entrypoint": "emptyTuple"
            },
            {
                "kind": "symtest",
                "entrypoint": "nestedTuples"
            },
            {
                "kind": "symtest",
                "entrypoint": "getIndexBailout"
            },
            {
                "kind": "symexec",
                "entrypoint": "getIndex",
                "expected": "1"
            },
            {
                "kind": "symexec",
                "entrypoint": "getIndexOpt",
                "expected": "bsqterm_none"
            }
        ]
    },
    {
        "src": "records.bsq",
