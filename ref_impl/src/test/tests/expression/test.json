[
    {
        "src": "literals.bsq",
        "tests": [
            {
                "kind": "compile"
            },
            {
                "kind": "aot",
                "entrypoint": "literalNone",
                "expected": "none"
            },
            {
                "kind": "aot",
                "entrypoint": "literalTrue",
                "expected": "true"
            },
            {
                "kind": "aot",
                "entrypoint": "literalFalse",
                "expected": "false"
            },
            {
                "kind": "aot",
                "entrypoint": "literal1",
                "expected": "1"
            },
            {
                "kind": "aot",
                "entrypoint": "literal3",
                "expected": "3"
            },
            {
                "kind": "aot",
                "entrypoint": "literalEmptyString",
                "expected": "\"\""
            },
            {
                "kind": "aot",
                "entrypoint": "literalHello",
                "expected": "\"hello\""
            },
            {
                "kind": "symtest",
                "entrypoint": "literal1"
            },
            {
                "kind": "symexec",
                "entrypoint": "literal1",
                "expected": "1"
            },
            {
                "kind": "symexec",
                "entrypoint": "literalFalse",
                "expected": "false"
            }
        ]
    },
    {
        "src": "tuples.bsq",
        "tests": [
            {
                "kind": "compile"
            },
            {
                "kind": "aot",
                "entrypoint": "emptyTuple",
                "expected": "[]"
            },
            {
                "kind": "aot",
                "entrypoint": "oneTuple",
                "expected": "[1]"
            },
            {
                "kind": "aot",
                "entrypoint": "fourTuple",
                "expected": "[1, 2, none, true]"
            },
            {
                "kind": "aot",
                "entrypoint": "nestedTuples",
                "expected": "[1, [none, []]]"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndex",
                "expected": "1"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexOpt",
                "expected": "none"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexBailout",
                "args": ["true"],
                "expected": "3"
            },
            {
                "kind": "aot",
                "entrypoint": "getIndexBailout",
                "args": ["false"],
                "expected": "none"
            },
            {
                "kind": "symtest",
                "entrypoint": "emptyTuple"
            },
            {
                "kind": "symtest",
                "entrypoint": "nestedTuples"
            },
            {
                "kind": "symtest",
                "entrypoint": "getIndexBailout"
            },
            {
                "kind": "symexec",
                "entrypoint": "getIndex",
                "expected": "1"
            },
            {
                "kind": "symexec",
                "entrypoint": "getIndexOpt",
                "expected": "bsqterm_none"
            }
        ]
    },
    {
        "src": "records.bsq",
        "tests": [
            {
                "kind": "compile"
            },
            {
                "kind": "aot",
                "entrypoint": "emptyRecord",
                "expected": "{}"
            },
            {
                "kind": "aot",
                "entrypoint": "oneRecord",
                "expected": "{f=1}"
            },
            {
                "kind": "aot",
                "entrypoint": "fourRecord",
                "expected": "{f=1, g=2, h=none, k=true}"
            },
            {
                "kind": "aot",
                "entrypoint": "nestedRecords",
                "expected": "{f=1, g={f=2, h={}}}"
            },
            {
                "kind": "aot",
                "entrypoint": "getField",
                "expected": "1"
            },
            {
                "kind": "aot",
                "entrypoint": "getFieldOpt",
                "expected": "none"
            },
            {
                "kind": "aot",
                "entrypoint": "getFieldBailout",
                "args": ["true"],
                "expected": "3"
            },
            {
                "kind": "aot",
                "entrypoint": "getFieldBailout",
                "args": ["false"],
                "expected": "none"
            },
            {
                "kind": "symtest",
                "entrypoint": "emptyRecord"
            },
            {
                "kind": "symtest",
                "entrypoint": "nestedRecords"
            },
            {
                "kind": "symtest",
                "entrypoint": "getFieldBailout"
            },
            {
                "kind": "symexec",
                "entrypoint": "getField",
                "expected": "1"
            },
            {
                "kind": "symexec",
                "entrypoint": "getFieldOpt",
                "expected": "bsqterm_none"
            }
        ]
    }
]